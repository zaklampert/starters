{
  "name": "react-webpack-ssr-starter",
  "description": "SSR/Webpack4/React",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Zak Lampert <zaklampert@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run clean && cross-env NODE_ENV=development node server/index.js",
    "start": "npm run clean && cross-env NODE_ENV=production node server/index.js",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && eslint --fix src server webpack",
    "clean": "rimraf buildClient buildServer",
    "precommit": "lint-staged",
    "cm": "git-cz"
  },
  "dependencies": {
    "colors": "^1.3.0",
    "express": "^4.16.3",
    "express-no-favicons": "^0.0.1",
    "extract-css-chunks-webpack-plugin": "^3.0.10",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.0-alpha.2",
    "sass-loader": "^7.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-flush-chunks": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "poststylus": "^1.0.0",
    "prettier": "^1.13.7",
    "react-hot-loader": "^4.3.3",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "travis-github-status": "^1.6.3",
    "webpack": "4.16.1",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
